<Window x:Name="Main_Window" x:Class="CDB_TSB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CDB_TSB"
        mc:Ignorable="d"
        Title="CDB Ticket Summary &amp; Breakdown" Height="950" Width="1090" Background="Gray">
    <Grid>
        <Grid x:Name="Results_Grid" HorizontalAlignment="Stretch" Margin="10,35,10,10" VerticalAlignment="Stretch" Background="LightGray">

        </Grid>
        <DatePicker x:Name="StartDate" HorizontalAlignment="Left" Margin="75,5,0,0" VerticalAlignment="Top" SelectedDateChanged="DatePicker_SelectedDateChanged" Keyboard.PreviewKeyUp="ReturnPressListener"/>
        <DatePicker x:Name="EndDate" HorizontalAlignment="Left" Margin="261,5,0,0" VerticalAlignment="Top" SelectedDateChanged="DatePicker_SelectedDateChanged" Keyboard.PreviewKeyUp="ReturnPressListener"/>
        <Button Content="Search" HorizontalAlignment="Left" Margin="387,7,0,0" VerticalAlignment="Top" Width="75" Click="SearchButton_Click"/>
        <Label Content="Start Date:" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top"/>
        <Label Content="End Date:" HorizontalAlignment="Left" Margin="200,4,0,0" VerticalAlignment="Top"/>   

    </Grid>
</Window>
